
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On

RewriteBase /

# bower_components允许直接访问, 不需要走php
RewriteRule wp/bower_components/(.+) bower_components/$1  [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [QSA,L]

</IfModule>
# END WordPress
